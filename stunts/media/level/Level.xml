<!-- this is a level -->
<level>

	<gravity value="9.81" />
	
	<terrain>
		<import root="terrain" file="Terrain1.xml" />
	</terrain>

	<objects>
		<object type="car">
			<name>HumanPlayerCar</name>
			<import file="CarBMW.xml" />
			<control name="Human" />
			
			<posX unit="gridUnits">10</posX>
			<posY unit="gridUnits" type="bottom">0</posY>
			<posZ unit="gridUnits">13</posZ>
	
		</object>

		<object type="car">
			<name>AICar</name>
			<import file="CarBMW.xml" />
			<control name="AICar" />
		</object>
		
		<!-- this imports all track objects -->
		<import root="track" file="total_track_objects.xml" />
	</objects>

	<atmosphere>
		<import root="atmosphere" file="Atmosphere1.xml" />
	</atmosphere>
</level>