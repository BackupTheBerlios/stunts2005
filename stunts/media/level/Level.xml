<!-- this is a level -->
<level>

	<gravity value="9.81" />

	<terrain>
		<import root="terrain" file="Terrain1.xml" />
	</terrain>

	<objects>
		<object type="car">
			<name>HumanPlayerCar</name>
			<import file="CarBMW.xml" />
			<control name="Human" />
		</object>

		<object type="car">
			<name>AICar</name>
			<import file="CarBMW.xml" />
			<!-- AICar is loaded from another file -->
			<control name="AICar" />
		</object>

		<!-- this imports all track objects -->
		<import root="track" file="Track1.xml" />
	</objects>

	<atmosphere>
		<import root="atmosphere" file="Atmosphere1.xml" />
	</atmosphere>
</level>